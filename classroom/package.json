{
  "name": "docuscope-classroom",
  "version": "4.0.0",
  "description": "Visualization of DocuScope analytics of a set of documents.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/CMU_Sidecar/docuscope-classroom.git"
  },
  "author": "Michael Ringenberg <ringenberg@cmu.edu>",
  "license": "CC BY-NC-SA 4.0",
  "bugs": {
    "url": "https://gitlab.com/CMU_Sidecar/docuscope-classroom/issues"
  },
  "homepage": "https://gitlab.com/CMU_Sidecar/docuscope-classroom#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --output-hashing none",
    "build_dev": "ng build --base-href=/classroom/ --output-hashing none",
    "build_prod": "ng build --base-href=/docuscope/classroom/ --output-hashing none --configuration=deploy",
    "test": "ng test",
    "test_headless": "ng test --progress=false --watch=false --browsers=ChromeHeadlessNoSandbox --code-coverage=true",
    "lint": "ng lint",
    "eslint": "eslint -c .eslintrc.js --ext .ts src",
    "codeclimate": "ng lint --format=json | node ./formatters/codeclimateFormatter.js > gl-codequality.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "angular-google-charts": "^1.1.4",
    "angular-tag-cloud-module": "^4.2.0",
    "core-js": "^3.6.5",
    "d3": "^5.16.0",
    "jquery": "^3.5.1",
    "ngx-ui-loader": "^9.1.1",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "rxjs-tslint": "^0.1.8",
    "service": "^0.1.4",
    "tslib": "^1.13.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.11",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "ajv": "^6.12.2",
    "ajv-keywords": "^3.4.1",
    "codelyzer": "^5.2.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.4",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "memoizee": "^0.4.14",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  }
}
