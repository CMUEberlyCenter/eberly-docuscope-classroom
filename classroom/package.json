{
  "name": "docuscope-classroom",
  "version": "2.1.1",
  "description": "Visualization of DocuScope analytics of a set of documents.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/CMU_Sidecar/docuscope-classroom.git"
  },
  "author": "Michael Ringenberg <ringenberg@cmu.edu>",
  "license": "CC BY-NC-SA 4.0",
  "bugs": {
    "url": "https://gitlab.com/CMU_Sidecar/docuscope-classroom/issues"
  },
  "homepage": "https://gitlab.com/CMU_Sidecar/docuscope-classroom#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot --prod --output-hashing none",
    "build_dev": "ng build --aot --prod --base-href=/classroom/ --output-hashing none",
    "build_prod": "ng build --aot --base-href=/docuscope/classroom/ --output-hashing none --configuration=deploy",
    "test": "ng test",
    "test_headless": "ng test --progress=false --watch=false --browsers=ChromeHeadlessNoSandbox --code-coverage=true",
    "lint": "ng lint",
    "codeclimate": "ng lint --format=json | node ./formatters/codeclimateFormatter.js > gl-codequality.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "angular-google-charts": "^0.1.6",
    "angular-tag-cloud-module": "^3.8.1",
    "core-js": "^3.4.6",
    "d3": "^5.14.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "ngx-ui-loader": "^8.0.0",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.5.3",
    "rxjs-tslint": "^0.1.7",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "^8.3.20",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.14",
    "ajv": "^6.10.2",
    "ajv-keywords": "^3.4.1",
    "codelyzer": "^5.2.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "memoizee": "^0.4.14",
    "protractor": "~5.4.2",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "typescript": "^3.5.3"
  }
}
