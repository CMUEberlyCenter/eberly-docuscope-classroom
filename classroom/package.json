{
  "name": "docuscope-classroom",
  "version": "5.1.0",
  "description": "Visualization of DocuScope analytics of a set of documents.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/CMU_Sidecar/docuscope-classroom.git"
  },
  "author": "Michael Ringenberg <ringenberg@cmu.edu>",
  "license": "CC BY-NC-SA 4.0",
  "bugs": {
    "url": "https://gitlab.com/CMU_Sidecar/docuscope-classroom/issues"
  },
  "homepage": "https://gitlab.com/CMU_Sidecar/docuscope-classroom#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-hashing none",
    "build:docuscope": "ng build --base-href=./ --output-hashing none",
    "build_dev": "ng build --base-href=/classroom/ --output-hashing none",
    "build:deploy": "ng build --base-href=/docuscope/classroom/ --output-hashing none --configuration=deploy",
    "test": "ng test --code-coverage=true",
    "test_headless": "ng test --progress=false --watch=false --browsers=ChromeHeadlessNoSandbox --code-coverage=true",
    "lint": "eslint -c .eslintrc.json src",
    "prettier": "prettier -w src",
    "e2e": "ng e2e",
    "version": "node ./getVersion.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "angular-google-charts": "^2.2.2",
    "angular-split": "^5.0.0",
    "core-js": "^3.16.0",
    "d3": "^7.0.0",
    "ngx-ui-loader": "^11.0.0",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "service": "^0.1.4",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.0",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "^12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@types/d3": "^7.0.0",
    "@types/google.visualization": "^0.0.67",
    "@types/jasmine": "^3.8.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "ajv": "^8.6.2",
    "ajv-keywords": "^5.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-gitlab": "^2.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
